# Database
DATABASE_URL=sqlite:///data/medical_dept.db

# Google APIs
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
GMAIL_CREDENTIALS_PATH=config/credentials/gmail_credentials.json
GOOGLE_FORMS_CREDENTIALS_PATH=config/credentials/forms_credentials.json

# LLM (Ollama)
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama3-elyza

# Application Settings
LOG_LEVEL=INFO
LOG_FILE_PATH=logs/app.log
BACKUP_DIR=backups/

# GA Settings (Default Values)
GA_POPULATION_SIZE=100
GA_GENERATIONS=200
GA_CROSSOVER_PROB=0.7
GA_MUTATION_PROB=0.2
GA_MISMATCH_BONUS=1.5

# System
FISCAL_YEAR=2025